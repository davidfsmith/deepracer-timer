<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>DeepRacer Timer</title>
    <link rel="stylesheet" href="/timer.css">
</head>

<body>
    <nav class="controls">
        <a href="#" id="btn_start" class="button btn_start">Start</a>
        <a href="#" id="btn_pause" class="button btn_pause">Pause</a>
        <a href="#" id="btn_reset" class="button btn_reset">Lap Reset</a>
        <a href="#" id="btn_clear" class="button btn_clear">Clear</a>
        <br/>
        <br/>
        <a href="#" id="btn_lap" class="button btn_lap">Lap</a>
        <a href="#" id="btn_car_reset" class="button btn_car_reset">Car Reset</a>
    </nav>
    <div class="limiter"></div>
    <div class="display"></div>
    <div class="bestlap_txt">Best:<span class="bestlap"></span></div>
    <div class="lastlap_txt">Last:<span class="lastlap"></span></div>
    <ul class="results"></ul>
</body>

</html>

<script src="/socket.io/socket.io.js"></script>
<script src="/timer.js"></script>
